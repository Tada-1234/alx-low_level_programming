#include <stdio.h>
#include "main.h"
/**
 * print_array - prints the elements of an array
 * @a: pointer to the array
 * @n: number of elements in the array
 *
 * Return: no return
 */
void print_array(int *a, int n)
{
	int i;

	for (i = 0; i < n; i++)
	{
		if (i < n - 1)
		{
			printf("%p, ", a[i]);
		}
		else
		{
			printf("%p", a[i]);
		}
	}
	printf("\n");
}
