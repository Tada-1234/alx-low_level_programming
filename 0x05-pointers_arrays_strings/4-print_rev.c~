#include "main.h"
#include <string.h>
/**
 * print_rev - prints the string in reverse
 * @s: holds the address of the string
 *
 * Return: has no return
 */
void print_rev(char *s)
{
	int i, j, len;

	i = 0;
	while(s[i] != '\0')
	{
		i++;
	}
	len = i;
	for (j = len - 1; j >= 0; j--)
	{
		_putchar(s[j]);
	}
	_putchar('\n');
}
